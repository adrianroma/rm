{
  "name": "runcore_front-end_app",
  "version": "1.0",
  "description": "Core font-end",
  "engines": {
    "node": "8.9.1"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "development": "npm run clean && BABEL_ENV=development webpack --config webpack.config.dev.js --colors --display-modules",
    "lint": "eslint src",
    "postmerge": "npm install",
    "postwrite": "npm install",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "production": "npm run clean && BABEL_ENV=production webpack --config webpack.config.prod.js --colors --display-modules && NODE_ENV=production node src/server",
    "start": "npm run development && NODE_ENV=development node src/server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "express": "^4.15.2",
    "material-ui": "^1.0.0-beta.35",
    "proptypes": "^1.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack": "^4.1.0",
    "webpack-dev-middleware": "^3.0.0",
    "webpack-hot-middleware": "^2.21.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.0.4",
    "husky": "^0.14.3",
    "react-hot-loader": "^4.0.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "style-loader": "^0.20.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexisNava/runcore"
  },
  "license": "MIT"
}
